<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <PackageId>HonestyDotNet</PackageId>
    <Version>0.7.2</Version>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Authors>Rajiv Singh</Authors>
    <Copyright>Rajiv Singh, 2022</Copyright>
    <PackageDescription>Provides generic Optional and Result monads to aid Functional Programming in C#.</PackageDescription>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/singh-rajiv/honesty-dotnet</RepositoryUrl>
  </PropertyGroup>
</Project>
